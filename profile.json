{
  "$jason": {
    "head": {
      "data": {
        "posts": [{
          "avatar": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/audrey_avatar.png",
          "name": "Audrey",
          "location": "Somewhere",
          "content": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/audrey.jpg"
        }, {
          "avatar": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/boss_avatar.png",
          "name": "Audrey",
          "location": "Somewhere",
          "content": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/boss.jpg"
        }, {
          "avatar": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/chet_avatar.png",
          "name": "Chet!",
          "location": "Somewhere",
          "content": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/chet.jpg"
        }, {
          "avatar": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/girl_avatar.png",
          "name": "Girl",
          "location": "Somewhere",
          "content": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/girl.jpg"
        }]
      },
      "templates": {
        "body": {
          "header": {
            "title": "barton",
            "menu": {
              "image": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/settings.png"
            }
          },
          "sections": [{
            "items": [{
              "type": "vertical",
              "style": {
                "align": "center",
                "padding": "20",
                "spacing": "10",
                "background": "#fbfbfb"
              },
              "components": [{
                "type": "label",
                "text": "Help Secure Your Account",
                "style": {
                  "font": "HelveticaNeue-Bold",
                  "size": "12"
                }
              }, {
                "type": "label",
                "text": "Confirm barton.fink@jasonette.com as your email address to help secure your account.",
                "style": {
                  "font": "HelveticaNeue",
                  "align": "center",
                  "size": "12",
                  "color": "#959597"
                }
              }, {
                "type": "button",
                "text": "Send Confirmation",
                "background": "#4592d8",
                "width": "300",
                "corner_radius": "5",
                "font": "HelveticaNeue-Bold",
                "size": "12",
                "color": "#ffffff"
              }, {
                "type": "label",
                "text": "Change Email Address",
                "color": "#4592d8",
                "font": "HelveticaNeue-Bold",
                "size": "12"
              }]
            }]
          }, {
            "items": {
              "{{#each posts}}": {
                "type": "vertical",
                "style": {
                  "padding": "0"
                },
                "components": [{
                  "type": "horizontal",
                  "style": {
                    "background": "#ffffff",
                    "padding": "9",
                    "spacing": "13"
                  },
                  "components": [{
                    "type": "image",
                    "url": "{{avatar}}",
                    "style": {
                      "width": "38",
                      "height": "38",
                      "corner_radius": "19"
                    }
                  }, {
                    "type": "vertical",
                    "components": [{
                      "type": "label",
                      "text": "{{name}}",
                      "style": {
                        "font": "HelveticaNeue-Bold",
                        "size": "12"
                      }
                    }, {
                      "type": "label",
                      "text": "{{location}} >",
                      "style": {
                        "font": "HelveticaNeue",
                        "size": "11"
                      }
                    }]
                  }, {
                    "type": "space"
                  }, {
                    "type": "image",
                    "url": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/dotdotdot.png",
                    "style": {
                      "padding_top": "10",
                      "width": "25"
                    }
                  }]
                }, {
                  "type": "image",
                  "url": "{{content}}",
                  "style": {
                    "width": "100%",
                    "padding": "0"
                  }
                }]
              }
            }
          }],
          "footer": {
            "tabs": {
              "style": {
                "background": "rgba(255,255,255,0.8)",
                "color": "#000000"
              },
              "items": [{
                "image": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/home.png",
                "style": {
                  "height": "21"
                },
                "url": "https://raw.githubusercontent.com/Jasonette/Instagram/master/home.json"
              }, {
                "image": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/search.png",
                "style": {
                  "height": "21"
                },
                "url": "https://raw.githubusercontent.com/Jasonette/Instagram/master/search.json"
              }, {
                "image": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/camera.png",
                "style": {
                  "height": "21"
                },
                "url": "https://raw.githubusercontent.com/Jasonette/Instagram/master/camera.json"
              }, {
                "image": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/heart.png",
                "style": {
                  "height": "21"
                },
                "url": "https://raw.githubusercontent.com/Jasonette/Instagram/master/heart.json"
              }, {
                "image": "https://raw.githubusercontent.com/Jasonette/Instagram/master/images/profile.png",
                "style": {
                  "height": "21"
                },
                "url": "https://raw.githubusercontent.com/Jasonette/Instagram/master/profile.json"
              }]
            }
          }
        }
      }
    }
  }
}
